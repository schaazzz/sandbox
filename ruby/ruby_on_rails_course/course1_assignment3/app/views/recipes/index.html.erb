<h1><%= @search_term %></h1>
<p>Powered By Food2Fork.com</p>

<table border="1">
  <tr>
    <th>Image</th>
    <th>Title</th>
    <th>Social Rank</th>
    <% @recipes.each do |recipe| %>
      <tr class = <%= cycle('even', 'odd') %>>
        <td><%= link_to image_tag(recipe["image_url"]), recipe["f2f_url"] %>
        <td><%= link_to(sanitize(recipe["title"]), recipe["f2f_url"]) %></td>
        <td><%= recipe["social_rank"] %></td>
      <tr>
    <% end %>
  <table>
